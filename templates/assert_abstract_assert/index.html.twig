{% extends 'base.html.twig' %}

{% block title %}AbstractAssert index{% endblock %}

{% block body %}
    <h1>AbstractAssert index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for abstract_assert in abstract_asserts %}
            <tr>
                <td>{{ abstract_assert.id }}</td>
                <td>
                    <a href="{{ path('assert_abstract_assert_show', {'id': abstract_assert.id}) }}">show</a>
                    <a href="{{ path('assert_abstract_assert_edit', {'id': abstract_assert.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('assert_abstract_assert_new') }}">Create new</a>
{% endblock %}